RadioMessage::RadioMessage(const char *name, short kind) : ::omnetpp::cPacket(name,kind)
{
    this->len = 0;
    for (unsigned int i=0; i<127; i++)
        this->data[i] = 0;
}

RadioMessage::RadioMessage(const RadioMessage& other) : ::omnetpp::cPacket(other)
{
    copy(other);
}

RadioMessage::~RadioMessage()
{
}

RadioMessage& RadioMessage::operator=(const RadioMessage& other)
{
    if (this==&other) return *this;
    ::omnetpp::cPacket::operator=(other);
    copy(other);
    return *this;
}

void RadioMessage::copy(const RadioMessage& other)
{
    this->len = other.len;
    for (unsigned int i=0; i<127; i++)
        this->data[i] = other.data[i];
}

void RadioMessage::parsimPack(omnetpp::cCommBuffer *b) const
{
    ::omnetpp::cPacket::parsimPack(b);
    doParsimPacking(b,this->len);
    doParsimArrayPacking(b,this->data,127);
}

void RadioMessage::parsimUnpack(omnetpp::cCommBuffer *b)
{
    ::omnetpp::cPacket::parsimUnpack(b);
    doParsimUnpacking(b,this->len);
    doParsimArrayUnpacking(b,this->data,127);
}

unsigned char RadioMessage::getLen() const
{
    return this->len;
}

void RadioMessage::setLen(unsigned char len)
{
    this->len = len;
}

unsigned int RadioMessage::getDataArraySize() const
{
    return 127;
}

unsigned char RadioMessage::getData(unsigned int k) const
{
    if (k>=127) throw omnetpp::cRuntimeError("Array of size 127 indexed by %lu", (unsigned long)k);
    return this->data[k];
}

void RadioMessage::setData(unsigned int k, unsigned char data)
{
    if (k>=127) throw omnetpp::cRuntimeError("Array of size 127 indexed by %lu", (unsigned long)k);
    this->data[k] = data;
}

